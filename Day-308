// Scala program to create a two-dimensional list
// using List collection

import scala.collection.immutable._

object Sample {
  // Main method
  def main(args: Array[String]) {
    val twoDList: List[List[Int]] =
      List(List(1, 2, 3), List(4, 5, 6), List(7, 8, 9), List(10, 20, 30))
    println("Two Dimension list: \n" + twoDList);
  }
}
