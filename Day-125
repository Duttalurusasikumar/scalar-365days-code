// Scala program to create an anonymous
// function with "=>" operator

object Sample {
  def main(args: Array[String]) {
    var result: Int = 0;

    // Create anonymous function.
    var AddNum = (num1: Int, num2: Int) => num1 + num2;

    // Function calling
    result = AddNum(20, 30);

    printf("Result : %d\n", result);
  }
}
