// Scala program to demonstrate the 
// final class

final class Demo1 {
  def sayHello() {
    println("Hello from Demo1");
  }
}

// Final class cannot be inherited.
// class Demo2 extends Demo1
class Demo2 {
  def sayHello() {
    println("Hello from Demo2");
  }
}

object Sample {
  def main(args: Array[String]) {
    var obj1 = new Demo1();
    var obj2 = new Demo2();

    obj1.sayHello();
    obj2.sayHello();
  }
}
